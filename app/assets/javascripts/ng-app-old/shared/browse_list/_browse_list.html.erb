<div class="container" ng-controller="BrowseCtrl as browse">

  <div class="row">
    <div class="col-md-12">
      <!--<paginator total-items="totalItems" items-per-page="itemsPerPage"></paginator>-->
      <pagination
      total-items="totalItems"
      items-per-page="itemsPerPage"
      ng-model="currentPage"
      ng-change="pageChanged()"
      boundary-links="true"
      max-size="10"
      previous-text="&lsaquo;"
      next-text="&rsaquo;"
      first-text="&laquo;"
      last-text="&raquo;">
      </pagination>
    </div>
  </div>

  <section class="row">
    <div class="col-md-12">
      <div class="panel panel-dark text-left">

        <div class="panel-heading" browse-panel-heading title="{{panelTitle}}" menuitems="panelMenuItems"></div>

        <div class="list-group" ui-view></div>

      </div>
    </div>
  </section>

  <div class="row">
    <div class="col-md-12">
      <!--<paginator total-items="totalItems" items-per-page="itemsPerPage"></paginator>-->
    </div>
    <pagination
    total-items="totalItems"
    items-per-page="itemsPerPage"
    ng-model="currentPage"
    ng-change="pageChanged()"
    boundary-links="true"
    max-size="10"
    previous-text="&lsaquo;"
    next-text="&rsaquo;"
    first-text="&laquo;"
    last-text="&raquo;">
    </pagination>
  </div>

</div>